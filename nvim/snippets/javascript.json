{
  "console.log": {
    "prefix": "cl",
    "body": "console.log(${0})"
  },
  "useState": {
    "prefix": "uses",
    "body": [
      "const [$1, set${1/(.*)/${1:/capitalize}/}] = useState($2);"
    ],
    "description": "creates use state"
  },
  "useEffect": {
    "prefix": "usee",
    "body": [
      "useEffect(() => {",
      "  ${2}",
      "}, [${1}])"
    ]
  },
  "Init Component": {
    "prefix": "rafce",
    "body": [
      "const $1 = () => {",
      "  return (",
      "    <div>",
      "      $2",
      "    </div>",
      "  )",
      "}",
      "",
      "export default $1"
    ]
  }
}
